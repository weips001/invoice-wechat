/* pages/billList/billList.wxss */
page {
  margin: 10rpx 0;
  background-color: #eee;
}
.wrap {
  width: 90%;
  margin: 0 auto;
}
.item {
  /* width: 90%; */
  background-color: #fff;
  border-radius: 8rpx;
  padding: 4rpx;
  margin-bottom: 20rpx;
}
.time {
  text-align: right;
  font-size: 10px;
}
.info {
  font-size: 12px;
}
.empty {
  height: 240rpx;
  margin: 24rpx;
  border-radius: 8rpx;
  background-color: #fff;
  text-align: center;
  line-height: 240rpx;
  font-size: 24px;
  color: #999
}